import random
num = random.randint(1, 20)
for i in range(5):
    guess = int(input("Guess a number (1–20): "))
    if guess == num:
        print("Correct! 🎯")
        break
    elif guess < num:
        print("Too low!")
    else:
        print("Too high!")
else:
    print("Sorry, the number was:", num)
